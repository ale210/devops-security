{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "iam:CreateAccessKey"
            ],
            "Resource": "arn:aws:iam::*:user/*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "iam:UpdateLoginProfile"
            ],
            "Resource": "arn:aws:iam::*:user/*",
            "Condition": {
                "StringEquals": {
                    "iam:ResourceTag/Access Level": ["1", "2"]
                }
            }
        }
    ]
}
